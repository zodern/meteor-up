{
  "name": "mup",
  "version": "1.2.2",
  "description": "Production Quality Meteor Deployments",
  "main": "lib/index.js",
  "bin": {
    "mup": "./index.js"
  },
  "scripts": {
    "prepublish": "npm run build -s",
    "build": "babel src --out-dir lib --sourceRoot=./ --copy-files --presets=es2015,stage-3",
    "build:watch": "npm run build -s -- -w",
    "test": "nofat test",
    "lint:defaultConfig": "eslint --ext=\".sample\" -c .eslintrc.yml --rule=\"comma-dangle: 0\" .",
    "lint": "eslint . && npm run lint:defaultConfig -s"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kadirahq/meteor-up.git"
  },
  "keywords": [
    "meteor"
  ],
  "author": "Kadira Inc.",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kadirahq/meteor-up/issues"
  },
  "homepage": "https://github.com/kadirahq/meteor-up#readme",
  "devDependencies": {
    "babel-preset-stage-3": "^6.22.0",
    "eslint": "^3.15.0",
    "nofat": "1.0.x"
  },
  "dependencies": {
    "archiver": "1.x.x",
    "babel-polyfill": "6.6.1",
    "bluebird": "3.x.x",
    "boxen": "^1.0.0",
    "chalk": "^1.1.3",
    "commander": "2.9.x",
    "debug": "2.x.x",
    "expand-tilde": "^2.0.2",
    "joi": "^10.2.1",
    "nodemiral": "1.x.x",
    "parse-json": "^2.2.0",
    "shelljs": "0.5.x",
    "silent-npm-registry-client": "2.x.x",
    "source-map-support": "0.4.0",
    "ssh2": "0.4.x",
    "underscore": "1.x.x",
    "uuid": "2.x.x"
  }
}
